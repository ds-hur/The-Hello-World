package com.javalec.main;

import com.javalec.function.SumEvenOdd;

public class MainClass {

	public static void main(String[] args) {
		// 객체지향 프로그래밍
		
		int startNum = 100, endNum = 200;
		
		SumEvenOdd sumEvenOdd  = new SumEvenOdd(); //클래스 이름, 인스턴스, 컨스트럭터(생성자) 이름
		int sum = sumEvenOdd.sumCalc(startNum, endNum);
		
		String result = sumEvenOdd.evenOdd(sum);

		System.out.println(sum);
		System.out.println(result);
		
		
		
	}

}
